package praktikum.pbo.pkg1;

import java.util.Scanner;
import javax.swing.JOptionPane;

/**
 *
 * @author Dell
 */
public class PraktikumPBO1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
      
    String name = JOptionPane.showInputDialog ("Input Nama Anda");
    String address = JOptionPane.showInputDialog ("Input Alamat Anda");
    int year =  Integer.parseInt (JOptionPane.showInputDialog ("Input Tahun Angkatan Anda"));
    JOptionPane.showMessageDialog (null, "Hallo Nama saya " + name);
    JOptionPane.showMessageDialog (null, "Alamat " + address );
    JOptionPane.showMessageDialog (null, "Angkatan " + year);

    
    }
       
}
